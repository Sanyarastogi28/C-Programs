/*1Program that checks whether the two numbers entered by the user are equal or not.*/

#include <stdio.h>

int main()
{
    int a,b;
    printf("Enter two numbers: ");
    scanf("%d%d",&a,&b);
    if(a==b)
    {
        printf("both are equal");
    }
    else
    {
        printf("not equal");
    }
}
